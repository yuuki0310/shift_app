<h1>ログイン</h1>
<%= form_for(:login, url: login_index_path) do |f| %>
  <%= f.label :email %>
  <%= f.email_field :email %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.submit "Log in" %>
<% end %>
<% if logged_in? %>
  <ul>
    <li><%= @current_user.id %></li>
    <li><%= session[:user_id] %></li>
  </ul>
<% end %>